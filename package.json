  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootstrap-vue/bootstrap-vue.git"
  },
  "files": [
    "src",
    "dist",
    "nuxt",
    "es"
  ],
  "sideEffects": [
    "./es/utils/object.js",
    "./src/utils/object.js",
    "./es/utils/array.js",
    "./src/utils/array.js",
    "**/*.scss",
    "**/*.css",
    "**/*.vue",
    "./docs"
  ],
  "scripts": {
    "build": "scripts/build.sh",
    "watch": "rollup -c scripts/rollup.config.js --watch",
    "docs-dev": "cross-env NODE_ENV=docs nuxt dev -c docs/nuxt.config.js",
    "docs-gen": "cross-env NODE_ENV=docs nuxt generate -c docs/nuxt.config.js",
    "docs-build": "cross-env NODE_ENV=docs nuxt build -c docs/nuxt.config.js",
    "docs-publish": "scripts/docs-publish.sh",
    "test": "jest",
    "lint": "eslint --ext .js,.vue ./",
    "prettify": "prettier --write '**/*.{js,json,md,scss,ts,vue}'",
    "release": "npm run build && npm run test && standard-version",
    "postinstall": "opencollective || exit 0"
  },
  "dependencies": {
    "@nuxt/opencollective": "^0.2.1",
    "bootstrap": "^4.3.1",
    "popper.js": "^1.14.7",
    "vue-functional-data-merge": "^2.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/standalone": "^7.3.4",
    "@nuxtjs/google-analytics": "^2.1.0",
    "@nuxtjs/pwa": "^3.0.0-beta.12",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.4.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-istanbul": "^5.1.1",
    "clean-css-cli": "^4.2.1",
    "codecov": "^3.2.0",
    "codemirror": "^5.44.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.11",
    "gh-pages": "^2.0.1",
    "highlightjs": "^9.12.0",
    "html-loader": "^0.5.5",
    "jest": "^24.1.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "marked": "^0.6.1",
    "node-sass": "^4.11.0",
    "nuxt": "^2.4.5",
    "postcss-cli": "^6.1.2",
    "prettier": "1.14.3",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-watch": "^4.3.1",
    "sass-loader": "^7.1.0",
    "standard-version": "^5.0.1",
    "terser": "^3.16.1",
    "vue": "^2.6.8",
    "vue-jest": "^3.0.4",
    "vue-server-renderer": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  },
  "jest": {
    "testRegex": "spec.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "coverageDirectory": "./coverage/",
    "testEnvironmentOptions": {
      "pretendToBeVisual": true
    }
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap 4",
    "Bootstrap for Vue",
