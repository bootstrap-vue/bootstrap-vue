{
  "title": "Form Input",
  "component": "bFormInput",
  "components": ["bFormInputStatic"],
  "events": [
    {
      "event": "input",
      "description": "Emitted when the input receives input from user.",
      "args": [
        {
          "arg": "value",
          "description": "current value of the input"
        }
      ]
    },
    {
      "event": "change",
      "description": "Emitted when the input changes.",
      "args": [
        {
          "arg": "value",
          "description": "current value of the input"
        }
      ]
    },
    {
      "event": "keyup",
      "description": "Native keyup event.",
      "args": [
        {
          "arg": "event",
          "description": "native event"
        }
      ]
    },
    {
      "event": "focus",
      "description": "When input receives focus."
    },
    {
      "event": "blur",
      "description": "When input loses focus."
    }
  ]
}
